<div id="<%= dom_id school %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= school.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Localization:</strong>
    <%= school.localization %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= school.description %>
  </p>

</div>
